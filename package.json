{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && copyfiles './src/**/*.graphql' dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generateTypes": "concurrently \"ts-node generate-typings\" \"onchange \"src/graphql.ts\" -- prettier --write --ignore-unknown {{changed}}\" ",
    "start": "nest start",
    "start:dev": "concurrently \"nest start --watch\" \"yarn generateTypes\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:db": "docker compose up",
    "dev": "yarn start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:restart": "rimraf dist/migrations"
  },
  "dependencies": {
    "@adminjs/express": "^5.1.0",
    "@adminjs/nestjs": "^5.1.1",
    "@adminjs/typeorm": "^4.0.0",
    "@apollo/server": "^4.5.0",
    "@aws-sdk/client-s3": "^3.290.0",
    "@aws-sdk/lib-storage": "^3.290.0",
    "@hubspot/api-client": "^6.0.1-beta.6",
    "@nestjs/apollo": "^11.0.3",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/graphql": "^11.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/mail": "^7.6.2",
    "@sentry/node": "^6.19.3",
    "@sentry/tracing": "^6.19.3",
    "@slack/web-api": "^6.7.1",
    "@types/passport-local": "^1.0.34",
    "adminjs": "^6.8.7",
    "analytics-node": "^6.0.0",
    "axios": "^0.25.0",
    "body-parser": "^1.19.2",
    "cli-color": "^2.0.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "dd-trace": "^2.3.0",
    "env-var": "^7.1.1",
    "express-formidable": "^1.2.0",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "flagsmith-nodejs": "^1.1.1",
    "graphql": "^16.6.0",
    "image-downloader": "^4.3.0",
    "jwks-rsa": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stripe": "^11.15.0",
    "ts-morph": "^17.0.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.3.9",
    "@types/cli-color": "^2.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jest-mock": "^27.4.6",
    "msw": "^0.36.7",
    "onchange": "^7.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
